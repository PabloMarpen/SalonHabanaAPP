<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/wall3">

    <TextView
        android:id="@+id/textView"
        android:layout_width="272dp"
        android:layout_height="108dp"
        android:layout_marginTop="56dp"
        android:fontFamily="@font/worksanssemibold"
        android:text="Datos enviados con exito"
        android:textAlignment="center"
        android:textColor="#FCAA20"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="313dp"
        android:layout_height="116dp"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/worksanssemibold"
        android:text="Gracias por confiar en Habana"
        android:textAlignment="center"
        android:textColor="#FCAA20"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="238dp"
        android:layout_height="217dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/rounded"
        app:layout_constraintEnd_toEndOf="@+id/textView"
        app:layout_constraintStart_toStartOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:srcCompat="@drawable/salonhabana" />


</androidx.constraintlayout.widget.ConstraintLayout>